<script type="text/javascript">
var sum=1+20;
console.log(sum);
document.write("1+20= "+sum+"<br>");

//함수도 가능
function cal(a,b){
	return a+b;
}
sum=cal(5,3);
document.write("cal(5,3)="+sum);
document.write("cal(7,3)="+sum(7,3));
</script>
